#!/usr/bin/env ruby
lib = File.expand_path('../../lib', __FILE__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)

require 'rubygems'
require 'bundler/setup'
require 'tiki/torch'
require 'fake_sqs/test_integration'

T  = Tiki::Torch
CS = Tiki::Torch::Consumer
E  = Tiki::Torch::Event
CB = Tiki::Torch::ConsumerBroker
P  = Tiki::Torch::Publisher
M  = Tiki::Torch::Monitoring

require_relative '../spec/support/constants'
require_relative '../spec/support/helpers'

require 'logger'

require 'pry'
ARGV.clear
Pry.start


