#!/usr/bin/env ruby
# -*- encoding: utf-8 -*-
lib = File.expand_path('../../lib', __FILE__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)

require 'rubygems'
require 'bundler/setup'
require 'tiki/torch'

T  = Tiki::Torch
CS = Tiki::Torch::Consumer
E  = Tiki::Torch::Event
EB = Tiki::Torch::EventBroker
EP = Tiki::Torch::EventProcessor
P  = Tiki::Torch::Publisher
SG = Tiki::Torch::SupervisionGroup

require 'logger'
Celluloid.logger = ::Logger.new STDOUT

require 'pry'
ARGV.clear
Pry.start


